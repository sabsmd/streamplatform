    $(document).ready(function() {

        var tempScrollTop,
            currentScrollTop = 0;
        var header = $('#header');
        $(window).scroll(function() {
            if ($(this).scrollTop() > 70) {
                header.addClass('sticky-hide')
            } else {
                header.removeClass('sticky-hide')
            }
            ;
            currentScrollTop = $(window).scrollTop();
            if (tempScrollTop < currentScrollTop) {
                header.removeClass('sticky')
            } else if (tempScrollTop > currentScrollTop) {
                header.addClass('sticky')
            }
            ;
            tempScrollTop = currentScrollTop;
        });

        if ($(window).width() > 1220) {
            $('.nav-menu li:has(.hidden-menu)').hover(function() {
                $(this).find('.hidden-menu').fadeToggle(200);
                $(this).toggleClass('menuactive');
            });
        } else {
            $('body').on('click', '.nav-menu li:has(.hidden-menu) > a', function() {
                $(this).parent().find('.hidden-menu').slideToggle(200);
                $(this).parent().toggleClass('menuactive');
                return false;
            });
        }
        ;
        $('.nav-menu li:has(ul)').addClass('submenu');

        $('.frate').each(function() {
            var a = $(this),
                aVotes = a.children('.fr-votes'),
                b = parseInt(aVotes.find('.ratingtypeplusminus').text()),
                c = parseInt(aVotes.find('span[id]:last').text());
            if (c >= b && c > 0) {
                var m = (c - b) / 2,
                    p = c - m,
                    pTen = (Math.round((c - (c - b) / 2) / c * 100)) / 10,
                    mTen = 10 - pTen;
            } else {
                var m = 0,
                    p = 0,
                    pTen = 0,
                    mTen = 0;
            }
            ;
            a.find('.ps').append('<span class="psc">' + p + '</span>');
            a.find('.ms').append('<span class="msc">' + m + '</span>');
            a.find('.fr-common').append('<div>' + pTen + '<div>' + titlesArr[1] + '</div></div>');
            a.find('.fr-plus').circleProgress({
                size: 80.0,
                startAngle: -1.5,
                lineCap: 'round',
                value: pTen / 10,
                emptyFill: '#1e1e1e',
                fill: {
                    gradient: ['#2dc225', '#33a42d']
                }
            });
            a.find('.fr-minus').circleProgress({
                size: 80.0,
                startAngle: -1.5,
                lineCap: 'round',
                value: mTen / 10,
                emptyFill: '#1e1e1e',
                fill: {
                    gradient: ['#f4470b', '#dd4d1b']
                }
            });
        });
    });

