                $targetBox = $(this).closest('.sect').children('.sect-c');
            $targetBox.html('<span class="fa fa-spinner fa-spin"></span>');
            $.post(dle_root + "engine/ajax/custom.php", {
                castom: $castom
            }, function(data) {
                $targetBox.html(data);
            });
            $(this).addClass('current').siblings().removeClass('current');
        });

    });
